{
  "name": "@snaptopixel/picto-beta",
  "version": "0.1.8",
  "description": "Preview, develop and document StencilJS components",
  "main": "dist/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "types": "dist/types/components.d.ts",
  "module": "dist/esm/es5/index.js",
  "bin": {
    "picto-init": "bin/picto"
  },
  "files": [
    "bin/",
    "dist/",
    "install/"
  ],
  "husky": {
    "hooks": {
      "post-merge": "yarn && stencil build --config stencil.dev.js --dev",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint \"src/**/*.ts?(x)\"",
    "pretty": "prettier --write \"src/**/*.ts?(x)\"",
    "build": "stencil build",
    "start": "stencil build --config stencil.dev.ts --dev --watch --serve --docs --docs-json docs/build/picto/components.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snaptopixel/picto-beta.git"
  },
  "keywords": [
    "stenciljs",
    "web-components",
    "stencil",
    "documentation"
  ],
  "author": "Casey Corcoran (https://github.com/snaptopixel)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/snaptopixel/picto-beta/issues"
  },
  "homepage": "https://github.com/snaptopixel/picto-beta#readme",
  "devDependencies": {
    "@stencil/core": "^0.17.0",
    "@stencil/router": "^0.3.2",
    "@types/bricks.js": "^1.8.0",
    "@types/classnames": "^2.2.7",
    "@types/color": "^3.0.0",
    "@types/highlight.js": "^9.12.3",
    "@types/js-yaml": "^3.12.0",
    "@types/lodash-es": "^4.17.1",
    "@types/marked": "^0.6.0",
    "bricks.js": "^1.8.0",
    "bulma": "^0.7.2",
    "classnames": "^2.2.6",
    "color": "^3.1.0",
    "emotion": "^10.0.6",
    "front-matter": "^3.0.1",
    "fuse.js": "^3.3.0",
    "highlight.js": "^9.13.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "lodash-es": "^4.17.11",
    "marked": "^0.6.0",
    "popper.js": "^1.14.6",
    "prettier": "^1.16.1",
    "rollup-plugin-alias": "^1.5.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-stencil": "^1.0.0"
  }
}
